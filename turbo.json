{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["codegen", "^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "codegen": {},
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputLogs": "new-only"
    },
    "lint": {},
    "typecheck": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  },
  "globalEnv": [],
  "globalPassThroughEnv": ["NODE_ENV", "CI"]
}
